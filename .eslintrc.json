{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
    "next/core-web-vitals",
    "plugin:react/recommended",
    "standard-with-typescript"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
      "react/react-in-jsx-scope": "off"
  }
}